import java.util.Scanner;
public class ReverseTheNumber 
{
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number: ");
		int n=sc.nextInt();
		int sum=0;
		while(n>0)
		{
			int rem=n%10;
			n=n/10;
			sum=sum*10+rem;
		}
		System.out.println("Reversed Number: "+sum);
	}
}
