public class SortArray 
{
	public static void main(String[] args) 
	{
		int arr[]= {11,2,6,9,-10,20,5};
		for(int i=0;i<arr.length;i++) 
		{
			for(int j=i+1;j<arr.length;j++){
				{
					if(arr[i]>arr[j])
					{
						int t=arr[i];
						arr[i]=arr[j];
						arr[j]=t;
					}
				}
			}
		}
		System.out.print("Sorted Array--->");
		for(int i=0;i<arr.length;i++)
		{
			System.out.print("  "+arr[i]);
		}
	}
}
